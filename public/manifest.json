{
    "manifest_version": 3,
    "name": "Fooocus Enhancer",
    "version": "1.0.0",
    "description": "A powerful Chrome extension that enhances the Fooocus web UI experience by adding useful features and improvements.",
    "permissions": ["storage"],
    "host_permissions": ["http://127.0.0.1:7865/*"],
    "content_scripts": [{
        "matches": ["http://127.0.0.1:7865/*"],
        "js": ["content.js"],
        "css": ["css/main.css"],
        "run_at": "document_idle"
    }],
    "action": {
        "default_icon": {
            "16": "icons/icon-16.png",
            "48": "icons/icon-48.png",
            "128": "icons/icon-128.png"
        }
    }
}
